{
  "name": "purescript-ocelot",
  "version": "0.21.1",
  "private": true,
  "scripts": {
    "clean": "rm -rfd output .spago node_modules dist/*.css dist/*.js",
    "build": "yarn run build:ui && yarn run build:css",
    "build:ui": "spago --config ./ui-guide/spago.dhall bundle-app --main Main --to dist/index.js",
    "build:css": "tailwindcss build ./css/index.css -o ./dist/cn-tailwind.css",
    "serve:ui": "serve ./dist"
  },
  "devDependencies": {
    "serve": "^11.3.2",
    "tailwindcss": "^1.6.2"
  },
  "dependencies": {
    "big-integer": "^1.6.48",
    "request": "^2.88.2"
  }
}
